{
  "name": "my-qwik-basic-starter",
  "description": "Demo App with Routing built-in (recommended)",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "_dev": "vite --mode ssr",
    "_dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "dev": "cross-env NODE_ENV=development vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "1.1.5",
    "@builder.io/qwik-city": "1.1.5",
    "@types/eslint": "8.40.0",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "@typescript-eslint/parser": "5.59.8",
    "@vanilla-extract/css": "^1.11.0",
    "autoprefixer": "10.4.14",
    "cross-env": "7.0.3",
    "eslint": "8.41.0",
    "eslint-plugin-qwik": "1.1.5",
    "postcss": "8.4.23",
    "prettier": "2.8.8",
    "styled-vanilla-extract": "^0.5.4",
    "tailwindcss": "3.3.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "5.0.4",
    "undici": "5.22.1",
    "vite": "4.3.9",
    "vite-tsconfig-paths": "4.2.0"
  },
  "dependencies": {
    "pg": "8.11.0"
  }
}
